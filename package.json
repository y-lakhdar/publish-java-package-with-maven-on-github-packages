{
  "name": "@y-lakhdar/publish-java-package-with-maven-on-github-packages",
  "version": "1.0.24",
  "description": "Push API client",
  "type": "commonjs",
  "devDependencies": {
    "@coveo/semantic-monorepo-tools": "2.1.2",
    "@octokit/auth-action": "2.0.3",
    "conventional-changelog-angular": "5.0.13",
    "cz-conventional-changelog": "3.3.0",
    "lint-staged": "13.2.2",
    "octokit": "2.0.14",
    "standard-version": "9.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "commit": "git-cz",
    "standard-version": "standard-version",
    "bump-pom": "node bump-pom.js"
  },
  "standard-version": {
    "scripts": {
      "postbump": "node bump-pom.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y-lakhdar/publish-java-package-with-maven-on-github-packages.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/y-lakhdar/publish-java-package-with-maven-on-github-packages/issues"
  },
  "homepage": "https://github.com/y-lakhdar/publish-java-package-with-maven-on-github-packages#readme"
}
